{
	"coc.preferences.formatOnSaveFiletypes": [
		"typescript",
		"typescriptreact",
		"javascript",
		"javascriptreact",
		"solidity",
		"css",
		"json",
		"python",
		"go",
		"lua"
	],
	"python.linting.mypyEnabled": true,
	"python.linting.ruffEnabled": true,
	"python.formatting.provider": "black",
	"Lua.workspace.library": {
		"/opt/homebrew/Cellar/neovim/0.9.4/share/nvim/runtime/lua": true,
		"/opt/homebrew/Cellar/neovim/0.9.4/share/nvim/runtime/lua/vim": true,
		"/opt/homebrew/Cellar/neovim/0.9.4/share/nvim/runtime/lua/vim/lsp": true
	},
	"Lua.workspace.maxPreload": 2000,
	"Lua.workspace.preloadFileSize": 1000,
	"Lua.runtime.version": "LuaJIT",
	"Lua.diagnostics.globals": [
		"vim"
	],
	"Lua.telemetry.enable": false,
	"Lua.format.defaultConfig": {
		"indent_style": "space",
		"indent_size": "2"
	},
	"Lua.format.enable": true,
	"Lua.format.formatter": "stylua"
}
